<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="dsm.SchoolInfo" table="SchoolInfo" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="maxTimeToCancel" column="MaxTimeToCancel" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="startTime" column="StartTime" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="endTime" column="EndTime" type="integer" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
