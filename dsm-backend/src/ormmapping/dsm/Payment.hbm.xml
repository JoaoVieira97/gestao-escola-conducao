<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="dsm.Payment" table="Payment" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="secretary" cascade="lock" column="SecretaryUserID" class="dsm.Secretary" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="value" column="Value" type="double" not-null="true" lazy="false"/>
		<property name="timestamp" column="Timestamp" type="date" not-null="false" lazy="false"/>
		<property name="description" column="Description" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
