<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="dsm.Instructor" table="Instructor" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="firstName" column="FirstName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="lastName" column="LastName" type="string" length="255" not-null="false" lazy="false"/>
		<property name="email" column="Email" type="string" length="255" not-null="false" lazy="false"/>
		<property name="password" column="Password" type="string" length="255" not-null="false" lazy="false"/>
		<set name="ORM_PracticalLessons" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="InstructorID" not-null="true"/>
			<one-to-many class="dsm.PracticalLesson"/>
		</set>
		<set name="ORM_WorkingDays" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="InstructorID" not-null="true"/>
			<one-to-many class="dsm.WorkingDay"/>
		</set>
		<set name="ORM_TheoreticalLessons" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="InstructorID" not-null="true"/>
			<one-to-many class="dsm.TheoreticalLesson"/>
		</set>
	</class>
</hibernate-mapping>
