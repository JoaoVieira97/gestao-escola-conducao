<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="dsm.PracticalLesson" table="PracticalLesson" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="increment">
			</generator>
		</id>
		<property name="startTime" column="StartTime" type="date" not-null="false" lazy="false"/>
		<property name="duration" column="Duration" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="state" column="State" type="string" length="255" not-null="false" lazy="false"/>
		<many-to-one name="instructor" cascade="lock" column="InstructorID" class="dsm.Instructor" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="isStudentPresent" column="IsStudentPresent" type="boolean" length="1" not-null="true" lazy="false"/>
		<set name="ORM_Students" table="Student_PracticalLesson2" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="PracticalLessonID" not-null="true"/>
			<many-to-many column="StudentID" class="dsm.Student">
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>
